@model Myprotos.Customer

@{
    ViewData["Title"] = "Create Customer";
}

<h2>Create New Customer</h2>

<form asp-action="Create" method="post">
    <div>
        <label>ID</label>
        <input asp-for="Id" />
    </div>
    <div>
        <label>Name</label>
        <input asp-for="Name" />
    </div>
    <div>
        <label>Address</label>
        <input asp-for="Address" />
    </div>
    <button type="submit">Create</button>
</form>
